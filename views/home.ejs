                                        <% } else { %>
                                            <div class="status-offline">
                                                <%= user.last_online %>
                                            </div>
                                            <% } %>
                                </div>
                                <% if (index !=moderators.length - 1) { %>
                                    <div class="line"></div>
                                    <% } %>
                                        <% }); %>
                        </div>
                    </div>
