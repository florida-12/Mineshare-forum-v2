<body>
    <style>
        body {
            background: #fff;
            background: url('../img/cloud-bg.png') repeat-x 0 / 90% auto;
            animation: cloud-bg 60s linear infinite;
            margin: 0;
        }

        .container {
            width: auto;
        }

        @keyframes cloud-bg {
            0% {
                background-position: 0 -100px;
            }

            100% {
                background-position: 100vw -100px;
            }
        }

        nav {
            position: relative;
        }

        nav .navbar {
            height: 95px;
            position: relative;
            background: white;
            box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.1);
            border-radius: 12px;
            padding-left: 30px;
            padding-right: 30px;
            margin-top: 40px;
        }

        nav .navbar .logo {
            margin-top: 27px;
        }

        nav .navbar .logo a {
            color: #000;
            font-family: 'TT Norms Pro', sans-serif;
            text-transform: none;
            font-weight: 600;
            font-size: 28px;
            text-decoration: none;
        }

        nav .navbar .logo a .forum {
            color: #bdbdbe;
            font-weight: 500;
        }

        nav .navbar .search {
            height: 50px;
            background: #fff;
            box-shadow: 0 0 3px rgba(0, 0, 0, 0.15);
            border-radius: 10px;
            padding-left: 10px;
            padding-right: 10px;
            margin-top: 22.5px;
            margin-right: 12px;
            vertical-align: top;
        }

        nav .navbar .search input {
            height: 34px;
            border: none;
            font-family: 'TT Norms Pro', sans-serif;
            vertical-align: top;
            margin-top: 6px;
        }

        nav .navbar .search input:focus {
            outline: none;
        }

        nav .navbar .search button {
            width: 32px;
            height: 32px;
            position: relative;
            background: #11B2FF;
            border-radius: 6px;
            border: none;
            vertical-align: top;
            margin-top: 9px;
            cursor: pointer;
        }

        nav .navbar .search button i {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            color: #fff;
        }

        nav .navbar .auth .auth-button {
            width: 150px;
            height: 50px;
            background: linear-gradient(98.44deg, #8DEBFF 0%, #00ABFF 100%), #339DFF;
            box-shadow: 0px 0px 10px rgba(113, 212, 255, 0.7);
            border-radius: 10px;
            border: none;
            font-size: 19px;
            color: #fff;
            font-family: 'TTNormsMedium';
            font-size: 19px;
            padding-bottom: 4px;
            margin-top: 22.5px;
            cursor: pointer;
        }

        nav .navbar .auth .auth-button-mobile {
            width: 50px;
            height: 50px;
            position: relative;
            background: linear-gradient(98.44deg, #8DEBFF 0%, #00ABFF 100%), #339DFF;
            border-radius: 10px;
            border: none;
            font-size: 19px;
            color: #fff;
            margin-top: 22.5px;
            cursor: pointer;
        }

        nav .navbar .auth .auth-button-mobile i {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
        }

        .card {
            background-color: #F3F7FC;
            border-radius: 18px;
            padding-top: 25px;
            padding-left: 25px;
            padding-right: 25px;
            padding-bottom: 25px;
            margin-bottom: 25px;
        }

        .global-title {
            position: relative;
            color: rgb(198, 198, 198);
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 26px;
            font-weight: 500;
            margin-top: -5px;
            margin-left: 5px;
            margin-bottom: 20px;
        }

        .global-title::after {
            content: '';
            position: absolute;
            top: 55%;
            left: 155px;
            width: calc(100% - 160px);
            height: 2px;
            background: rgb(202, 202, 202);
            border-radius: 40px;
        }

        .fire-box {
            position: absolute;
            top: 50%;
            left: -46px;
            transform: translateY(-50%);
        }

        .fire {
            width: 30px;
            height: 30px;
            position: relative;
            background: #ff7400;
            border-radius: 6px;
        }

        .fire i {
            position: absolute;
            top: 27%;
            left: 30%;
            color: #fff;
            font-size: 14px;
        }

        .tip {
            width: 0;
            height: 0;
            position: absolute;
            top: 50%;
            left: 100%;
            transform: translateY(-50%);
            border-top: 5px solid transparent;
            border-bottom: 5px solid transparent;
            border-left: 6px solid #ff7400;
        }

        .topic {
            position: relative;
            background: #fff;
            background-size: 400px;
            background-repeat: no-repeat;
            background-position: 100%;
            box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.05);
            border-radius: 10px;
            padding-top: 20px;
            padding-left: 20px;
            padding-right: 20px;
            padding-bottom: 16px;
            margin-bottom: 20px;
            overflow: hidden;
            cursor: pointer;
            user-select: none;
        }

        .line {
            width: 100%;
            height: 2px;
            border-radius: 40px;
            background: rgb(202, 202, 202);
            margin-top: 30px;
            margin-bottom: 30px;
        }

        .topic:hover .title {
            text-decoration: underline;
        }

        .topic .avatar {
            width: 50px;
            height: 50px;
            position: relative;
            background: #11B2FF;
            border-radius: 6px;
            margin-right: 14px;
        }

        .topic .avatar i {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            color: #fff;
            font-size: 24px;
        }

        .topic .title-subtitle {
            height: 50px;
            vertical-align: top;
        }

        .topic .title {
            color: #3A4E57;
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 18px;
            font-weight: 700;
            margin-top: -1px;
            margin-bottom: 5px;
        }

        .topic .subtitle {
            width: 400px;
            color: #959595;
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 12px;
            font-weight: 400;
            line-height: 12px;
        }

        .topic .counter {
            height: 52.5px;
            overflow: hidden;
            vertical-align: top;
            float: right;
        }

        .topic .counter .records {
            height: 22px;
            background: #fff;
            border: 1.5px solid #ededed;
            border-radius: 4px;
            color: #9c9c9c;
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 13px;
            font-weight: 500;
            text-align: right;
            padding-top: 2px;
            padding-left: 10px;
            padding-right: 10px;
            padding-bottom: 2px;
            margin-bottom: 5px;
        }

        .topic .counter .records i {
            font-size: 10px;
            vertical-align: top;
            margin-top: 3.5px;
            margin-left: 5px;
        }

        .topic .counter .views {
            height: 22px;
            background: #fff;
            border: 1.5px solid #ededed;
            border-radius: 4px;
            color: #9c9c9c;
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 13px;
            text-align: right;
            font-weight: 500;
            padding-top: 2px;
            padding-left: 10px;
            padding-right: 10px;
            padding-bottom: 2px;
        }

        .topic .counter .views i {
            font-size: 10px;
            vertical-align: top;
            margin-top: 3px;
            margin-left: 3px;
        }

        .topic .info {
            width: 50px;
            height: 50px;
            position: relative;
            background: #fff;
            border: 1.5px solid #ededed;
            border-radius: 4px;
            float: right;
            margin-left: 7px;
            transition: 0.2s;
        }

        .topic .info:hover {
            border: 1.5px solid #11B2FF;
        }

        .topic .info i {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            color: #11B2FF;
            font-size: 26px;
        }

        .picture {
            position: relative;
            box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.05);
            border: 4px solid #fff;
            border-radius: 10px;
            overflow: hidden;
            margin-bottom: 20px;
        }

        .picture img {
            width: 100%;
            height: 300px;
            height: auto;
            margin-bottom: -6px;
        }

        .monitoring {
            background: #fff;
            box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.05);
            border-radius: 10px;
            padding-top: 20px;
            padding-left: 20px;
            padding-right: 20px;
            padding-bottom: 20px;
            margin-bottom: 20px;
            user-select: none;
        }

        .monitoring button {
            width: 100%;
            background: linear-gradient(98.44deg, #8DEBFF 0%, #00ABFF 100%), #339DFF;
            box-shadow: 0px 0px 8px rgba(113, 212, 255, 0.5);
            border-radius: 8px;
            border: none;
            color: #fff;
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 16px;
            font-weight: 600;
            line-height: 4px;
            padding-top: 14px;
            padding-left: 14px;
            padding-bottom: 14px;
            cursor: pointer;
        }

        .monitoring button:hover {
            background: linear-gradient(98.44deg, #76c1e5 0%, #008ccc 100%), #267bb7;
        }

        .monitoring button i {
            margin-left: 7px;
            font-size: 13px;
        }

        .moderation {
            background: #fff;
            box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.05);
            border-radius: 10px;
            padding-top: 20px;
            padding-left: 20px;
            padding-right: 20px;
            padding-bottom: 16px;
        }

        .moderation .user {
            position: relative;
        }

        .moderation .user .info {
            vertical-align: top;
        }

        .moderation .user .avatar {
            width: 30px;
            height: 30px;
            position: relative;
            border-radius: 3px;
            overflow: hidden;
            margin-right: 6px;
        }

        .moderation .user .avatar img {
            width: 100%;
            height: auto;
        }

        .moderation .user .username {
            color: #3A4E57;
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 14px;
            font-weight: 600;
            margin-top: -1px;
        }

        .moderation .user .major {
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 11px;
            font-weight: 500;
            margin-top: -1px;
        }

        .moderation .user .moder {
            color: #0F9D58;
        }

        .moderation .user .admin {
            color: #cc1f1f;
        }

        .moderation .user .status-online {
            position: absolute;
            top: -2px;
            right: 0;
            color: #49de3f;
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 10px;
            font-weight: 500;
        }

        .moderation .user .status-offline {
            position: absolute;
            top: -2px;
            right: 0;
            color: #b9b9b9;
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 10px;
            font-weight: 500;
        }

        .moderation .line {
            width: 100%;
            height: 2px;
            border-radius: 40px;
            background: #ededed;
            margin-top: 8px;
            margin-bottom: 12px;
        }

        footer {
            position: relative;
            background: #fff;
            box-shadow: 0px -2px 8px rgba(0, 0, 0, 0.08);
            padding-top: 60px;
            padding-bottom: 60px;
            margin-top: 25px;
        }

        footer .group {
            color: #3A4E57;
            font-family: 'TT Norms Pro', sans-serif;
            font-size: 18px;
            font-weight: 600;
            margin-bottom: 15px;
        }

        footer .link {
            font-family: "Nunito Sans", sans-serif;
            font-size: 14px;
            font-weight: 600;
            margin-bottom: 15px;
        }

        footer .link a {
            color: #808080;
        }

        footer .link a:hover {
            color: #11B2FF;
        }

        footer .discord {
            width: 40px;
            height: 40px;
            position: relative;
            background: #7289da;
            border: 2px solid #7289da;
            border-radius: 8px;
            cursor: pointer;
        }

        footer .discord:hover {
            border: 2px solid #11B2FF;
        }

        footer .discord i {
            position: absolute;
            top: 49%;
            left: 50%;
            transform: translate(-50%, -50%);
            color: #fff;
            font-size: 18px;
        }

        @media (max-width: 575.98px) {
            nav .navbar {
                padding-left: 25px;
                padding-right: 25px;
                margin-top: 20px;
            }

            nav .navbar .logo {
                margin-top: 15.5px;
            }

            nav .navbar .logo a .forum {
                font-weight: 600;
            }
        }
    </style>

